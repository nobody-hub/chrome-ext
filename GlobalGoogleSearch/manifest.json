{
  "name": "Global Google Search",
  "description": "Use the context menu to search a different contry's Google",
  "version": "1.0",
  "options_page": "options.html",
  "permissions": [
    "contextMenus",
    "storage"
  ],
  "background": {
    "scripts":[
      "locales.js",
      "background.js"
    ],
    "persistent":false
  },
  "browser_action": {
    "default_popup": "options.html"
  },
  "icons": {
    "16":"globalGoogle16.png",
    "48":"globalGoogle48.png",
    "128":"globalGoogle128.png"
  },
  "manifest_version": 2
}