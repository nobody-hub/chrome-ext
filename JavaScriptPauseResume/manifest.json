{
  "name": "JavaScript pause/resume",
  "description": "Pause/resumes Javascript execution",
  "version": "0.7",
  "permissions": [
    "debugger"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon" : "pause.png",
    "default_title": "Pause JaveScript"
  },
  "manifest_version": 2
}